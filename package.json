{
  "name": "plugins",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky && npx ts-patch install",
    "setup": "npx rimraf node_modules package-lock.json dist tmp yarn.lock && yarn",
    "start": "nps",
    "add": "nx g @nativescript/plugin-tools:add-package",
    "add-angular": "nx g @nativescript/plugin-tools:add-angular",
    "config": "nx g @nativescript/plugin-tools:config",
    "publish-packages": "nx g @nativescript/plugin-tools:publish",
    "sync-packages-with-demos": "nx g @nativescript/plugin-tools:sync-packages-with-demos",
    "remove-package": "nx g @nativescript/plugin-tools:remove-package",
    "add-demo": "nx g @nativescript/plugin-tools:add-demo"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.0",
    "@angular-eslint/eslint-plugin": "^19.0.0",
    "@angular-eslint/eslint-plugin-template": "^19.0.0",
    "@angular-eslint/template-parser": "^19.0.0",
    "@angular/animations": "^19.0.0",
    "@angular/cli": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/compiler-cli": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/language-service": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@eslint/eslintrc": "^2.1.1",
    "@nativescript/angular": "^19.0.0",
    "@nativescript/core": "~8.8.0",
    "@nativescript/plugin-tools": "5.5.1",
    "@nativescript/types": "~8.8.0",
    "@nativescript/webpack": "~5.0.24",
    "@ngrx/effects": "19.1.0",
    "@ngrx/store": "19.1.0",
    "@ngrx/store-devtools": "19.1.0",
    "@ngtools/webpack": "^19.0.0",
    "@types/jest": "27.0.2",
    "@types/jsan": "^3.1.2",
    "@types/socketcluster-client": "^13.0.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "husky": "~9.0.0",
    "jest": "27.2.3",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "11.1.1",
    "jsan": "^3.1.14",
    "nativescript-permissions": "1.3.11",
    "nativescript-vue": "~2.9.0",
    "nativescript-vue-template-compiler": "~2.9.0",
    "ng-packagr": "^19.0.0",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.1",
    "rxjs": "~7.8.0",
    "socketcluster-client": "^14.3.2",
    "ts-node": "10.9.1",
    "typescript": "~5.6.0",
    "typescript-eslint": "^8.19.0",
    "zone.js": "~0.15.0"
  },
  "lint-staged": {
    "**/*.{js,ts,scss,json,html}": [
      "nx format:write --files",
      "nx affected:lint --fix --files"
    ]
  },
  "dependencies": {
    "@ngrx/operators": "^19.0.0"
  }
}
